{
  "arrowParens": "always",
  "requireConfig": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
